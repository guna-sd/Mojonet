struct Constants:
    """Commonly used mathematical constants."""
    alias e: Float64 = 2.7182818284590451
    """`e` for the mathematical constant Euler's number (approximately 2.71828)."""

    alias pi: Float64 = 3.1415926535897931
    """`pi` is the ratio of the circumference of a circle to its diameter, approximately 3.14159."""

    alias tau: Float64 = 6.2831853071795862
    """`tau` is the ratio of the circumference of a circle to its radius, equal to 2Ï€, approximately 6.28319."""

    alias hfpi: Float64 = 1.5707963267948966
    """`hfpi` is half of `pi`, approximately 1.57080."""

    alias sqrthfpi: Float64 = 1.2533141373155001
    """`sqrthfpi` is the square root of half of `pi`, approximately 1.25331."""

    alias phi: Float64 = 1.6180339887498949
